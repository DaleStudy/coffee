{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types"],
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"allowImportingTsExtensions": true
	},
	"include": ["src/**/*"]
}
